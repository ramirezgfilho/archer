script/source = "extends CharacterBody2D

signal vida_terminou

@export var DAMAGE_RATE = 15.0
@export var SPEED = 100.0
@export var RUN_SPEED = 110.0
@export var stamina_recovery_rate = 1.0

var basic_bow = preload(\"res://cenas/basic_bow.tscn\")

var health = 100.0
var stamina = 100.0
var max_stamina = 100.0
var max_health = 100.0

func _physics_process(delta):
	var direction = Input.get_vector(\"move_left\",\"move_right\",\"move_up\",\"move_down\")
	if Input.get_action_raw_strength(\"run\", true):
		if stamina > 0:
			velocity = direction * RUN_SPEED
			stamina -= 1
			move_and_slide()
	velocity = direction * SPEED
	move_and_slide()
	%StaminaBar.value = stamina
		
	var mobs_encostando = %HurtBox.get_overlapping_bodies()
	if mobs_encostando.size() > 0:
		health -= DAMAGE_RATE * mobs_encostando.size() * delta
		%Health.value = health
	
	if health <= 0.0: 
		vida_terminou.emit()
	
	if velocity.length() <= 0.0:
		%CPUParticles2D.emitting = false
		%AnimatedSprite2D.play(\"idle_r\")

	##Única forma que consegui de animar as diversas posições
	if velocity.length() > 0.0:
		%CPUParticles2D.emitting = true 
		print(velocity)
		
		##Baixo
		if velocity.y > 0:
			%AnimatedSprite2D.play(\"walk_r\")
		if velocity.y < 0: ^^ 
			%AnimatedSprite2D.play(\"walk_up_r\")
		if velocity.x < 0: 
			%AnimatedSprite2D.play(\"walk_l\")
		if velocity.x > 0: 
			%AnimatedSprite2D.play(\"walk_r\")
		
			
##Fução para recuperação da Stamina
func _on_stamina_recover_timeout():
	if stamina < max_stamina:
		stamina += stamina_recovery_rate 

func _on_collect_weapon_box_area_entered(area):
	if area.TIPO_ITEM == \"Cogumelo\":
		if health < max_health:
			health += 20
			print(\"Coletou cogumelo e a vida é: \", health)
			if health > max_health:
				health = max_health
				print(health)
	print(health)

func aumenta_vida():
	max_health += 10
	print(\"A vida máxima do jogador é: \", max_health)
	
func aumenta_stamina():
	max_stamina += 10
	print(\"A stamina do jogador é: \", max_stamina)

func aumenta_recuperacao_stamina():
	stamina_recovery_rate += 5
	print(\"A taxa de recuperação de stamina é: \", stamina_recovery_rate)
	
func aumenta_velocidade_movimento():
	SPEED += 5
	print(\"A velocidade de movimento é: \", SPEED)
"

